-- ========== VARIABLES ==========
local selectedTarget = nil
local selectedTool = nil
local powerValue = 500
local minPower, maxPower = 100, 5000
local spawnCount = 5  -- —Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–æ, –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å slider –ø–æ–∑–∂–µ

-- ========== FUNCTIONS ==========
local function clearScroll(scroll)
    for _, child in pairs(scroll:GetChildren()) do
        if child:IsA("TextButton") then child:Destroy() end
    end
    scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
end

local function updateTargets()
    clearScroll(TargetsScroll)
    local yPos = 0
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= Player and p.Character then
            local btn = Instance.new("TextButton")
            btn.Name = p.Name
            btn.Parent = TargetsScroll
            btn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
            btn.BorderSizePixel = 0
            btn.Position = UDim2.new(0, 5, 0, yPos)
            btn.Size = UDim2.new(1, -10, 0, 30)
            btn.Font = Enum.Font.Gotham
            btn.Text = p.Name
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.TextScaled = true
            local tCorner = Instance.new("UICorner", btn); tCorner.CornerRadius = UDim.new(0, 6)
            
            btn.MouseButton1Click:Connect(function()
                selectedTarget = p
                print("üéØ –í—ã–±—Ä–∞–Ω–∞ —Ü–µ–ª—å: " .. p.Name)
                -- –û–±–Ω–æ–≤–∏—Ç—å tools –¥–ª—è —ç—Ç–æ–π —Ü–µ–ª–∏? –ù–µ—Ç, tools –æ–±—â–∏–µ
            end)
            
            yPos = yPos + 35
        end
    end
    TargetsScroll.CanvasSize = UDim2.new(0, 0, 0, yPos)
end

local function updateTools()
    clearScroll(ToolsScroll)
    local toolsFolder = ReplicatedStorage:FindFirstChild("Tools")
    if not toolsFolder then
        local noTools = Instance.new("TextLabel")
        noTools.Parent = ToolsScroll
        noTools.BackgroundTransparency = 1
        noTools.Size = UDim2.new(1, 0, 1, 0)
        noTools.Font = Enum.Font.Gotham
        noTools.Text = "Tools –Ω–µ –Ω–∞–π–¥–µ–Ω—ã (–ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∏ —Å–∫—Ä–∏–ø—Ç)"
        noTools.TextColor3 = Color3.fromRGB(255, 100, 100)
        noTools.TextScaled = true
        return
    end
    
    local yPos = 0
    print("üõ†Ô∏è –î–æ—Å—Ç—É–ø–Ω—ã–µ Tools:", toolsFolder:GetChildren())
    for _, tool in pairs(toolsFolder:GetChildren()) do
        if tool:IsA("Tool") or tool:IsA("Model") then  -- —Ç–∏–ø–∏—á–Ω—ã–µ tools
            local btn = Instance.new("TextButton")
            btn.Name = tool.Name
            btn.Parent = ToolsScroll
            btn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
            btn.BorderSizePixel = 0
            btn.Position = UDim2.new(0, 5, 0, yPos)
            btn.Size = UDim2.new(1, -10, 0, 30)
            btn.Font = Enum.Font.Gotham
            btn.Text = tool.Name  -- –ë–æ–º–±–∞, –†–∞–∫–µ—Ç–∞, –í–æ–∑–¥—É—à–Ω—ã–π —à–∞—Ä –∏ —Ç.–¥.
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.TextScaled = true
            local tCorner = Instance.new("UICorner", btn); tCorner.CornerRadius = UDim.new(0, 6)
            
            btn.MouseButton1Click:Connect(function()
                selectedTool = tool
                print("üõ†Ô∏è –í—ã–±—Ä–∞–Ω–∞ –∏–≥—Ä—É—à–∫–∞: " .. tool.Name)
            end)
            
            yPos = yPos + 35
        end
    end
    ToolsScroll.CanvasSize = UDim2.new(0, 0, 0, yPos)
end

local function activate()
    if not selectedTarget or not selectedTarget.Character or not selectedTarget.Character:FindFirstChild("HumanoidRootPart") then
        print("‚ùå –í—ã–±–µ—Ä–∏ —Ü–µ–ª—å!")
        return
    end
    if not selectedTool then
        print("‚ùå –í—ã–±–µ—Ä–∏ –∏–≥—Ä—É—à–∫—É!")
        return
    end
    
    local myRoot = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
    if myRoot then
        -- –¢–ü –∑–∞ —Å–ø–∏–Ω—É —Ü–µ–ª–∏
        myRoot.CFrame = selectedTarget.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -8)
        print("üß≤ –¢–ü –∫ " .. selectedTarget.Name)
    end
    
    -- –°–ø–∞–≤–Ω spawnCount tools –≤–æ–∑–ª–µ —Ü–µ–ª–∏ —Å —Ñ–ª–∏–Ω–≥–æ–º
    local targetPos = selectedTarget.Character.HumanoidRootPart.Position
    local camLook = WS.CurrentCamera.CFrame.LookVector
    for i = 1, spawnCount do
        local toolClone = selectedTool:Clone()
        toolClone.Parent = WS
        
        local primary = toolClone.PrimaryPart or toolClone:FindFirstChildWhichIsA("BasePart") or toolClone:FindFirstChild("Handle")
        if primary then
            -- –ü–æ–∑–∏—Ü–∏—è: –≤–æ–∑–ª–µ —Ü–µ–ª–∏ —Ä–∞–Ω–¥–æ–º–Ω–æ
            primary.CFrame = CFrame.new(targetPos + Vector3.new(math.random(-10,10), 5 + math.random(0,10), math.random(-10,10)))
            
            -- –§–ª–∏–Ω–≥ —Å power
            local bv = Instance.new("BodyVelocity")
            bv.MaxForce = Vector3.new(4e5, 4e5, 4e5)
            bv.Velocity = (camLook * powerValue * 1.5) + Vector3.new(0, powerValue * 0.5, 0)
            bv.Parent = primary
            
            Debris:AddItem(bv, 3)
            Debris:AddItem(toolClone, 10)  -- –∞–≤—Ç–æ-—É–¥–∞–ª–µ–Ω–∏–µ
        end
    end
    print("üöÄ –ê–ö–¢–ò–í–ê–¶–ò–Ø! " .. spawnCount .. "x " .. selectedTool.Name .. " –Ω–∞ " .. selectedTarget.Name .. " (Power: " .. powerValue .. ")")
end

-- Slider Logic
local dragging = false
SliderBtn.MouseButton1Down:Connect(function()
    dragging = true
end)
UIS.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)
RS.Heartbeat:Connect(function()
    if dragging then
        local mousePos = UIS:GetMouseLocation()
        local trackPos = SliderTrack.AbsolutePosition.X
        local trackSize = SliderTrack.AbsoluteSize.X
        local relPos = math.clamp((mousePos.X - trackPos) / trackSize, 0, 1)
        SliderBtn.Position = UDim2.new(relPos, -10, 0, 0)
        
        powerValue = math.floor(minPower + (maxPower - minPower) * relPos)
        PowerValueLabel.Text = tostring(powerValue)
    end
end)

-- ========== EVENTS ==========
CloseBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
end)

ActivateBtn.MouseButton1Click:Connect(activate)

-- Toggle Menu
UIS.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.Insert then
        MainFrame.Visible = not MainFrame.Visible
        if MainFrame.Visible then
            updateTargets()
            updateTools()
        end
    elseif input.KeyCode == Enum.KeyCode.F then
        -- Fly toggle (–∫–æ–¥ –∏–∑ –ø—Ä–æ—à–ª—ã—Ö —Å–∫—Ä–∏–ø—Ç–æ–≤)
        toggleFly()  -- –û–ø—Ä–µ–¥–µ–ª–∏ —Ñ—É–Ω–∫—Ü–∏—é –∫–∞–∫ —Ä–∞–Ω—å—à–µ
    elseif input.KeyCode == Enum.KeyCode.X then
        toggleNoclip()
    end
end)

-- Auto-update lists
spawn(function()
    while true do
        if MainFrame.Visible then
            updateTargets()
            updateTools()
        end
        wait(5)
    end
end)

-- ========== FLY & NOCLIP (–∫–æ—Ä–æ—Ç–∫–æ, –≤—Å—Ç–∞–≤—å –ø–æ–ª–Ω—ã–π –∫–æ–¥ –∏–∑ –ø—Ä–µ–¥—ã–¥—É—â–µ–≥–æ) ==========
-- –û–ø—Ä–µ–¥–µ–ª–∏ toggleFly(), toggleNoclip() –∫–∞–∫ –≤ –ø–µ—Ä–≤–æ–º —Å–∫—Ä–∏–ø—Ç–µ
-- SuperFling —Ö—É–∫ –µ—Å–ª–∏ –Ω—É–∂–Ω–æ

-- Noclip init
toggleNoclip()  -- –≤–∫–ª –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é

print("‚úÖ HUB –ì–û–¢–û–í! | Insert = –ú–µ–Ω—é | F=fly | X=noclip")
print("üëâ –í –º–µ–Ω—é: –í—ã–±–µ—Ä–∏ —Ü–µ–ª—å ‚Üí –ò–≥—Ä—É—à–∫—É ‚Üí Power ‚Üí –ê–ö–¢–ò–í–ê–¢–ò–†–û–í–ê–¢–¨!")
print("üõ†Ô∏è Tools –∞–≤—Ç–æ-–∏–∑ ReplicatedStorage.Tools (–±–æ–º–±—ã, —Ä–∞–∫–µ—Ç—ã, —à–∞—Ä—ã –∏ —Ç.–¥.)")
